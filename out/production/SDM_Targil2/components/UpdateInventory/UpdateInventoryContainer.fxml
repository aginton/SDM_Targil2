<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.UpdateInventory.UpdateInventoryContainerController">
   <children>
      <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="165.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="436.0" minWidth="10.0" prefWidth="435.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" percentHeight="33.0" prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints minHeight="10.0" percentHeight="33.0" prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints minHeight="10.0" percentHeight="34.0" prefHeight="30.0" vgrow="NEVER" />
        </rowConstraints>
         <children>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#onAddButtonAction" text="Add" />
               </children>
            </StackPane>
            <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="1">
               <children>
                  <Button fx:id="updateButton" mnemonicParsing="false" onAction="#onUpdateButtonAction" text="Update" />
               </children>
            </StackPane>
            <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="2">
               <children>
                  <Button fx:id="removeButton" mnemonicParsing="false" onAction="#onRemoveButtonAction" text="Remove" />
               </children>
            </StackPane>
            <AnchorPane fx:id="childAnchorPane" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowSpan="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
