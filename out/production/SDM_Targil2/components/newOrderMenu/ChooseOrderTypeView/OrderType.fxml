   <children>
      <Label layoutX="43.0" layoutY="66.0" text="Choose Order Type:" />
      <RadioButton fx:id="staticOrderRatioButton" layoutX="43.0" layoutY="144.0" mnemonicParsing="false" text="Static Order">
         <toggleGroup>
