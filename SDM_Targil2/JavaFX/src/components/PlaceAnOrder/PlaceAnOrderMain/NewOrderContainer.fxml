<AnchorPane prefHeight="102.0" prefWidth="601.0" stylesheets="@NewOrderContainerCSS.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.PlaceAnOrder.PlaceAnOrderMain.NewOrderContainerController">
