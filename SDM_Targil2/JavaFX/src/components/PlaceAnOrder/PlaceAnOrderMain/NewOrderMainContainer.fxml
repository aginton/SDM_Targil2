<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.PlaceAnOrder.PlaceAnOrderMain.NewOrderMainContainerController">
   <center>
      <AnchorPane fx:id="newOrderCurrentPage" />
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="64.0" prefWidth="605.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonAction" text="Back" />
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#nextButtonAction" text="Next" />
            <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#confirmButtonAction" text="Confirm" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
